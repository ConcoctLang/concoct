# This is a test file written in Concoct.

use testlib # import testlib

func add(a, b)
{
  a + b # last statement of a function is evaluated and the value is implicitly returned
}

intro = "This is a Concoct test program."
x = 5
y = 3
put(intro)     # outputs string defined for intro variable
put(add(x, y)) # outputs 8
